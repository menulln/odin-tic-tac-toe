{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "no-use-before-define": "off",
        "quotes": [
            "error",
            "single"
        ],
        "indent": [
            "error",
            "tab",
            { "SwitchCase": 1 }
        ]
    }
}
